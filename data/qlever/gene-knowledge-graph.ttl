<https://gene.ai/ontology/datasource> a owl:Ontology ;
    rdfs:label "Gene Data Source Ontology" ;
    dct:created "2026-01-25"^^xsd:date ;
    dct:creator "Kurt Cagle" ;
    dct:description "This ontology provides a formal model for cataloging biomedical data sources used in the Gene project. It covers data source hierarchy, versioning, access methods, licensing, schema definitions, and cross-references between sources." ;
    rdfs:comment "An OWL 2 ontology for describing biomedical data sources, their metadata, versions, access methods, licensing, and cross-references." ;
    owl:imports <http://www.w3.org/2004/02/skos/core> ;
    owl:versionInfo "1.0.0" .

ds:ActiveSource a owl:Class ;
    rdfs:label "Active Source" ;
    rdfs:comment "Data source with active documentation status." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:hasValue tax:Active ;
                        owl:onProperty ds:status ] ) ] .

ds:ApiAccessibleSource a owl:Class ;
    rdfs:label "API Accessible Source" ;
    rdfs:comment "Data source with REST API access available." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:onProperty ds:hasAccessMethod ;
                        owl:someValuesFrom [ a owl:Restriction ;
                                owl:hasValue tax:RestApi ;
                                owl:onProperty ds:accessMethodType ] ] ) ] .

ds:DeprecatedSource a owl:Class ;
    rdfs:label "Deprecated Source" ;
    rdfs:comment "Data source with deprecated status." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:hasValue tax:Deprecated ;
                        owl:onProperty ds:status ] ) ] .

ds:DirectDownloadMethod a ds:AccessMethod ;
    rdfs:label "Direct Download Access" ;
    rdfs:comment "Direct HTTP file download." ;
    ds:accessMethodType tax:DirectDownload .

ds:FtpMethod a ds:AccessMethod ;
    rdfs:label "FTP Download Access" ;
    rdfs:comment "FTP bulk download." ;
    ds:accessMethodType tax:FtpDownload .

ds:OpenAccessSource a owl:Class ;
    rdfs:label "Open Access Source" ;
    rdfs:comment "Data source with open access licensing (no commercial restrictions)." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:onProperty ds:hasLicense ;
                        owl:someValuesFrom [ a owl:Restriction ;
                                owl:hasValue true ;
                                owl:onProperty ds:commercialUseAllowed ] ] ) ] .

ds:RestApiMethod a ds:AccessMethod ;
    rdfs:label "REST API Access" ;
    rdfs:comment "RESTful web service interface." ;
    ds:accessMethodType tax:RestApi .

ds:SparqlAccessibleSource a owl:Class ;
    rdfs:label "SPARQL Accessible Source" ;
    rdfs:comment "Data source with SPARQL endpoint available." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:onProperty ds:hasAccessMethod ;
                        owl:someValuesFrom [ a owl:Restriction ;
                                owl:hasValue tax:SparqlEndpoint ;
                                owl:onProperty ds:accessMethodType ] ] ) ] .

ds:SparqlMethod a ds:AccessMethod ;
    rdfs:label "SPARQL Endpoint Access" ;
    rdfs:comment "SPARQL query endpoint for RDF data." ;
    ds:accessMethodType tax:SparqlEndpoint .

ds:Tier1Source a owl:Class ;
    rdfs:label "Tier 1 Source" ;
    rdfs:comment "Primary authoritative data source (tier 1)." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:hasValue tax:Tier1 ;
                        owl:onProperty ds:tier ] ) ] .

ds:Tier2Source a owl:Class ;
    rdfs:label "Tier 2 Source" ;
    rdfs:comment "Secondary supplementary data source (tier 2)." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:hasValue tax:Tier2 ;
                        owl:onProperty ds:tier ] ) ] .

ds:Tier3Source a owl:Class ;
    rdfs:label "Tier 3 Source" ;
    rdfs:comment "Specialized niche data source (tier 3)." ;
    owl:equivalentClass [ owl:intersectionOf ( ds:DataSource [ a owl:Restriction ;
                        owl:hasValue tax:Tier3 ;
                        owl:onProperty ds:tier ] ) ] .

ds:attributionText a owl:DatatypeProperty ;
    rdfs:label "attribution text" ;
    rdfs:comment "Recommended attribution text to use." ;
    rdfs:domain ds:License ;
    rdfs:range xsd:string .

ds:citationText a owl:DatatypeProperty ;
    rdfs:label "citation text" ;
    rdfs:comment "Preferred citation format for academic use." ;
    rdfs:domain ds:License ;
    rdfs:range xsd:string .

ds:complementedBy a owl:ObjectProperty ;
    rdfs:label "complemented by" ;
    rdfs:comment "Links a data source to complementary sources that provide additional data." ;
    rdfs:domain ds:DataSource ;
    rdfs:range ds:DataSource .

ds:created a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "created" ;
    rdfs:comment "Date when the resource was created." ;
    rdfs:range xsd:date .

ds:currentVersion a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "current version" ;
    rdfs:comment "Links a data source to its current/latest version." ;
    rdfs:domain ds:DataSource ;
    rdfs:range ds:Version ;
    rdfs:subPropertyOf ds:hasVersion .

ds:documentationUrl a owl:DatatypeProperty ;
    rdfs:label "documentation URL" ;
    rdfs:comment "URL to documentation or user guides." ;
    rdfs:range xsd:anyURI .

ds:formatType a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "format type" ;
    rdfs:comment "Links a format to its type from the taxonomy." ;
    rdfs:domain ds:Format ;
    rdfs:range skos:Concept .

ds:hasCrossReference a owl:ObjectProperty ;
    rdfs:label "has cross-reference" ;
    rdfs:comment "Links a data source to cross-references with other sources." ;
    rdfs:domain ds:DataSource ;
    rdfs:range ds:CrossReference .

ds:hasDataQualityNote a owl:ObjectProperty ;
    rdfs:label "has data quality note" ;
    rdfs:comment "Links a data source to data quality notes." ;
    rdfs:domain ds:DataSource ;
    rdfs:range ds:DataQualityNote .

ds:hasDataSource a owl:ObjectProperty ;
    rdfs:label "has data source" ;
    rdfs:comment "Links a subcategory to its child data sources." ;
    rdfs:domain ds:Subcategory ;
    rdfs:range ds:DataSource ;
    owl:inverseOf ds:belongsToSubcategory .

ds:hasFormat a owl:ObjectProperty ;
    rdfs:label "has format" ;
    rdfs:comment "Links a data source or access method to available data formats." ;
    rdfs:domain [ owl:unionOf ( ds:DataSource ds:AccessMethod ) ] ;
    rdfs:range ds:Format .

ds:hasIdentifierMapping a owl:ObjectProperty ;
    rdfs:label "has identifier mapping" ;
    rdfs:comment "Links a cross-reference to identifier mappings." ;
    rdfs:domain ds:CrossReference ;
    rdfs:range ds:IdentifierMapping .

ds:hasSubcategory a owl:ObjectProperty ;
    rdfs:label "has subcategory" ;
    rdfs:comment "Links a category to its child subcategories." ;
    rdfs:domain ds:Category ;
    rdfs:range ds:Subcategory ;
    owl:inverseOf ds:belongsToCategory .

ds:mappingCount a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "mapping count" ;
    rdfs:comment "Number of identifier mappings available." ;
    rdfs:domain ds:CrossReference ;
    rdfs:range xsd:long .

ds:parentLevel a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "parent level" ;
    rdfs:comment "Generic parent relationship in the hierarchy." ;
    rdfs:domain ds:HierarchyLevel ;
    rdfs:range ds:HierarchyLevel .

ds:previousVersion a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "previous version" ;
    rdfs:comment "Links a version to its predecessor." ;
    rdfs:domain ds:Version ;
    rdfs:range ds:Version .

ds:relatedTo a owl:ObjectProperty,
        owl:SymmetricProperty ;
    rdfs:label "related to" ;
    rdfs:comment "A symmetric relationship between related data sources." ;
    rdfs:domain ds:DataSource ;
    rdfs:range ds:DataSource .

ds:shortDescription a owl:DatatypeProperty,
        owl:FunctionalProperty ;
    rdfs:label "short description" ;
    rdfs:comment "Brief one-line description suitable for listings." ;
    rdfs:range xsd:string .

ds:sourceIdentifier a owl:ObjectProperty ;
    rdfs:label "source identifier" ;
    rdfs:comment "The identifier type in the source data source." ;
    rdfs:domain ds:IdentifierMapping ;
    rdfs:range ds:Identifier .

ds:subcategoryClassification a owl:FunctionalProperty,
        owl:ObjectProperty ;
    rdfs:label "subcategory classification" ;
    rdfs:comment "Links a subcategory to its SKOS concept in the taxonomy." ;
    rdfs:domain ds:Subcategory ;
    rdfs:range skos:Concept .

ds:supersedes a owl:ObjectProperty ;
    rdfs:label "supersedes" ;
    rdfs:comment "Indicates this source supersedes or replaces another." ;
    rdfs:domain ds:DataSource ;
    rdfs:range ds:DataSource .

ds:targetIdentifier a owl:ObjectProperty ;
    rdfs:label "target identifier" ;
    rdfs:comment "The identifier type in the target data source." ;
    rdfs:domain ds:IdentifierMapping ;
    rdfs:range ds:Identifier .

shapes:CategoryShape a sh:NodeShape ;
    rdfs:label "Category Shape" ;
    rdfs:comment "Validates category instances." ;
    sh:property [ rdfs:comment "Category identifier (e.g., 'genetics.genomics')." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Category must have an id in format 'domain.subdomain'." ;
            sh:minCount 1 ;
            sh:name "id" ;
            sh:path ds:id ;
            sh:pattern "^[a-z]+\\.[a-z]+$" ],
        [ rdfs:comment "Human-readable category title." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Category must have a title." ;
            sh:minCount 1 ;
            sh:name "title" ;
            sh:path ds:title ],
        [ rdfs:comment "Numeric notation (e.g., '01', '02')." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Category notation should be two digits." ;
            sh:name "notation" ;
            sh:path ds:notation ;
            sh:pattern "^[0-9]{2}$" ],
        [ rdfs:comment "SKOS concept link." ;
            sh:class skos:Concept ;
            sh:maxCount 1 ;
            sh:message "Category must be linked to a SKOS concept." ;
            sh:minCount 1 ;
            sh:name "classification" ;
            sh:path ds:categoryClassification ] ;
    sh:targetClass ds:Category .

shapes:CrossReferenceShape a sh:NodeShape ;
    rdfs:label "Cross-Reference Shape" ;
    rdfs:comment "Validates cross-reference definitions between sources." ;
    sh:property [ rdfs:comment "The target data source being referenced." ;
            sh:class ds:DataSource ;
            sh:maxCount 1 ;
            sh:message "Cross-reference must specify target source." ;
            sh:minCount 1 ;
            sh:name "targetSource" ;
            sh:path ds:referencesSource ],
        [ rdfs:comment "Qualitative coverage level." ;
            sh:datatype xsd:string ;
            sh:in ( "high" "medium" "low" ) ;
            sh:maxCount 1 ;
            sh:message "Coverage should be 'high', 'medium', or 'low'." ;
            sh:name "coverage" ;
            sh:path ds:coverageLevel ;
            sh:severity sh:Info ],
        [ rdfs:comment "Percentage of records with this cross-reference." ;
            sh:datatype xsd:decimal ;
            sh:maxCount 1 ;
            sh:maxInclusive 100 ;
            sh:message "Coverage percentage must be between 0 and 100." ;
            sh:minInclusive 0 ;
            sh:name "coveragePercent" ;
            sh:path ds:coveragePercentage ;
            sh:severity sh:Info ] ;
    sh:targetClass ds:CrossReference .

shapes:DataSourceShape a sh:NodeShape ;
    rdfs:label "Data Source Shape" ;
    rdfs:comment "Validates data source instances with required and optional properties." ;
    sh:property [ rdfs:comment "Unique identifier for the data source (lowercase, hyphen-separated)." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:maxLength 50 ;
            sh:message "Data source must have exactly one id (2-50 chars, lowercase alphanumeric with hyphens/dots)." ;
            sh:minCount 1 ;
            sh:minLength 2 ;
            sh:name "id" ;
            sh:path ds:id ;
            sh:pattern "^[a-z0-9][a-z0-9.-]*[a-z0-9]$" ],
        [ rdfs:comment "Keyword tags for categorization and search." ;
            sh:datatype xsd:string ;
            sh:maxLength 30 ;
            sh:message "Tags should be lowercase alphanumeric with hyphens." ;
            sh:minLength 2 ;
            sh:name "tags" ;
            sh:path ds:tag ;
            sh:pattern "^[a-z0-9-]+$" ;
            sh:severity sh:Info ],
        [ rdfs:comment "Detailed description of the data source." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Data source should have a description (at least 50 characters)." ;
            sh:minLength 50 ;
            sh:name "description" ;
            sh:path ds:description ;
            sh:severity sh:Warning ],
        [ rdfs:comment "Organization maintaining the data source." ;
            sh:class ds:Maintainer ;
            sh:message "Data source should specify a maintainer." ;
            sh:name "maintainer" ;
            sh:path ds:maintainedBy ;
            sh:severity sh:Warning ],
        [ rdfs:comment "Official website URL." ;
            sh:datatype xsd:anyURI ;
            sh:maxCount 1 ;
            sh:message "Data source should have a valid website URL (http or https)." ;
            sh:name "website" ;
            sh:path ds:website ;
            sh:pattern "^https?://" ;
            sh:severity sh:Warning ],
        [ rdfs:comment "How often the data source is updated." ;
            sh:class skos:Concept ;
            sh:maxCount 1 ;
            sh:message "Consider specifying update frequency." ;
            sh:name "updateFrequency" ;
            sh:path ds:updateFrequency ;
            sh:severity sh:Info ],
        [ rdfs:comment "Date documentation was last updated." ;
            sh:datatype xsd:date ;
            sh:maxCount 1 ;
            sh:message "Data source should have a last updated date." ;
            sh:name "lastUpdated" ;
            sh:path ds:lastUpdated ;
            sh:severity sh:Warning ],
        [ rdfs:comment "Available versions of the data source." ;
            sh:class ds:Version ;
            sh:message "Data source should have at least one version documented." ;
            sh:name "versions" ;
            sh:path ds:hasVersion ;
            sh:severity sh:Warning ],
        [ rdfs:comment "Human-readable title of the data source." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:maxLength 100 ;
            sh:message "Data source must have exactly one title (2-100 characters)." ;
            sh:minCount 1 ;
            sh:minLength 2 ;
            sh:name "title" ;
            sh:path ds:title ],
        [ rdfs:comment "License terms for the data source." ;
            sh:class ds:License ;
            sh:maxCount 1 ;
            sh:message "Data source must have license information." ;
            sh:name "license" ;
            sh:node shapes:LicenseShape ;
            sh:path ds:hasLicense ;
            sh:severity sh:Violation ],
        [ rdfs:comment "Available access methods." ;
            sh:class ds:AccessMethod ;
            sh:message "Data source should have at least one access method." ;
            sh:name "accessMethods" ;
            sh:node shapes:AccessMethodShape ;
            sh:path ds:hasAccessMethod ;
            sh:severity sh:Warning ],
        [ rdfs:comment "Data schema definition." ;
            sh:class ds:Schema ;
            sh:maxCount 1 ;
            sh:message "Data source should have a schema definition." ;
            sh:name "schema" ;
            sh:node shapes:SchemaShape ;
            sh:path ds:hasSchema ;
            sh:severity sh:Warning ],
        [ rdfs:comment "Identifier types used by this source." ;
            sh:class ds:Identifier ;
            sh:message "Consider documenting identifier types." ;
            sh:name "identifiers" ;
            sh:node shapes:IdentifierShape ;
            sh:path ds:hasIdentifier ;
            sh:severity sh:Info ],
        [ rdfs:comment "Primary use cases." ;
            sh:class ds:UseCase ;
            sh:message "Consider documenting use cases." ;
            sh:name "useCases" ;
            sh:path ds:hasUseCase ;
            sh:severity sh:Info ],
        [ rdfs:comment "Known limitations." ;
            sh:class ds:Limitation ;
            sh:message "Consider documenting limitations." ;
            sh:name "limitations" ;
            sh:path ds:hasLimitation ;
            sh:severity sh:Info ],
        [ rdfs:comment "Scheduled release day." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Consider specifying scheduled release day." ;
            sh:name "releaseDay" ;
            sh:path ds:releaseDay ;
            sh:severity sh:Info ],
        [ rdfs:comment "URL for update notifications." ;
            sh:datatype xsd:anyURI ;
            sh:maxCount 1 ;
            sh:message "Consider providing notification URL for updates." ;
            sh:name "notificationUrl" ;
            sh:path ds:notificationUrl ;
            sh:pattern "^https?://" ;
            sh:severity sh:Info ],
        [ rdfs:comment "Bidirectional integration partners." ;
            sh:class ds:DataSource ;
            sh:message "Consider documenting integration partners." ;
            sh:name "integrationPartner" ;
            sh:path ds:integrationPartner ;
            sh:severity sh:Info ],
        [ rdfs:comment "Parent category of the data source." ;
            sh:class ds:Category ;
            sh:maxCount 1 ;
            sh:message "Data source must belong to exactly one category." ;
            sh:minCount 1 ;
            sh:name "category" ;
            sh:path ds:belongsToCategory ],
        [ rdfs:comment "Parent subcategory of the data source." ;
            sh:class ds:Subcategory ;
            sh:maxCount 1 ;
            sh:message "Data source must belong to exactly one subcategory." ;
            sh:minCount 1 ;
            sh:name "subcategory" ;
            sh:path ds:belongsToSubcategory ],
        [ rdfs:comment "Documentation lifecycle status." ;
            sh:class skos:Concept ;
            sh:in ( tax:Draft tax:Active tax:Deprecated ) ;
            sh:maxCount 1 ;
            sh:message "Data source must have exactly one status (draft, active, or deprecated)." ;
            sh:minCount 1 ;
            sh:name "status" ;
            sh:path ds:status ],
        [ rdfs:comment "Quality and priority tier classification." ;
            sh:class skos:Concept ;
            sh:in ( tax:Tier1 tax:Tier2 tax:Tier3 ) ;
            sh:maxCount 1 ;
            sh:message "Data source should have a tier classification (1, 2, or 3)." ;
            sh:name "tier" ;
            sh:path ds:tier ;
            sh:severity sh:Warning ] ;
    sh:sparql [ sh:message "Data source must have at least one version OR be in draft status." ;
            sh:select """
        PREFIX ds: <https://gene.ai/ontology/datasource#>
        PREFIX tax: <https://gene.ai/taxonomy/>

        SELECT $this
        WHERE {
            $this a ds:DataSource .
            FILTER NOT EXISTS { $this ds:hasVersion ?version }
            FILTER NOT EXISTS { $this ds:status tax:Draft }
        }
    """ ;
            sh:severity sh:Warning ],
        [ sh:message "Active data source should have license information." ;
            sh:select """
        PREFIX ds: <https://gene.ai/ontology/datasource#>
        PREFIX tax: <https://gene.ai/taxonomy/>

        SELECT $this
        WHERE {
            $this a ds:DataSource ;
                  ds:status tax:Active .
            FILTER NOT EXISTS { $this ds:hasLicense ?license }
        }
    """ ;
            sh:severity sh:Warning ] ;
    sh:targetClass ds:DataSource .

shapes:LimitationShape a sh:NodeShape ;
    rdfs:label "Limitation Shape" ;
    rdfs:comment "Validates limitation descriptions." ;
    sh:property [ rdfs:comment "Description of the limitation." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Limitation must have a description (at least 10 characters)." ;
            sh:minCount 1 ;
            sh:minLength 10 ;
            sh:name "description" ;
            sh:path ds:description ] ;
    sh:targetClass ds:Limitation .

shapes:MaintainerShape a sh:NodeShape ;
    rdfs:label "Maintainer Shape" ;
    rdfs:comment "Validates maintainer/organization information." ;
    sh:property [ rdfs:comment "Organization name." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Maintainer must have a name." ;
            sh:minCount 1 ;
            sh:name "name" ;
            sh:path ds:title ],
        [ rdfs:comment "Organization website." ;
            sh:datatype xsd:anyURI ;
            sh:maxCount 1 ;
            sh:message "Consider providing organization website." ;
            sh:name "website" ;
            sh:path ds:website ;
            sh:pattern "^https?://" ;
            sh:severity sh:Info ] ;
    sh:targetClass ds:Maintainer .

shapes:SubcategoryShape a sh:NodeShape ;
    rdfs:label "Subcategory Shape" ;
    rdfs:comment "Validates subcategory instances." ;
    sh:property [ rdfs:comment "Subcategory identifier." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Subcategory must have an id." ;
            sh:minCount 1 ;
            sh:name "id" ;
            sh:path ds:id ],
        [ rdfs:comment "Human-readable subcategory title." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Subcategory must have a title." ;
            sh:minCount 1 ;
            sh:name "title" ;
            sh:path ds:title ],
        [ rdfs:comment "Parent category." ;
            sh:class ds:Category ;
            sh:maxCount 1 ;
            sh:message "Subcategory must belong to exactly one category." ;
            sh:minCount 1 ;
            sh:name "category" ;
            sh:path ds:belongsToCategory ],
        [ rdfs:comment "Numeric notation (e.g., '1.1', '2.3')." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Subcategory notation should be in format 'X.Y'." ;
            sh:name "notation" ;
            sh:path ds:notation ;
            sh:pattern "^[0-9]+\\.[0-9]+$" ] ;
    sh:sparql [ sh:message "Subcategory must have at least one data source." ;
            sh:select """
        PREFIX ds: <https://gene.ai/ontology/datasource#>

        SELECT $this
        WHERE {
            $this a ds:Subcategory .
            FILTER NOT EXISTS {
                ?source ds:belongsToSubcategory $this
            }
        }
    """ ;
            sh:severity sh:Info ] ;
    sh:targetClass ds:Subcategory .

shapes:UseCaseShape a sh:NodeShape ;
    rdfs:label "Use Case Shape" ;
    rdfs:comment "Validates use case descriptions." ;
    sh:property [ rdfs:comment "Description of the use case." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Use case must have a description (at least 10 characters)." ;
            sh:minCount 1 ;
            sh:minLength 10 ;
            sh:name "description" ;
            sh:path ds:description ] ;
    sh:targetClass ds:UseCase .

shapes:VersionShape a sh:NodeShape ;
    rdfs:label "Version Shape" ;
    rdfs:comment "Validates version instances with release information." ;
    sh:property [ rdfs:comment "Version identifier string." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Version must have a version number." ;
            sh:minCount 1 ;
            sh:minLength 1 ;
            sh:name "version" ;
            sh:path ds:versionNumber ],
        [ rdfs:comment "Date the version was released." ;
            sh:datatype xsd:date ;
            sh:maxCount 1 ;
            sh:message "Version must have a release date." ;
            sh:minCount 1 ;
            sh:name "releaseDate" ;
            sh:path ds:releaseDate ],
        [ rdfs:comment "Checksum for integrity verification (format: 'algorithm:hash')." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Checksum should be in format 'algorithm:hash' (e.g., 'sha256:abc123...')." ;
            sh:name "checksum" ;
            sh:path ds:checksum ;
            sh:pattern "^(sha256|sha512|md5):[a-f0-9]+$" ;
            sh:severity sh:Info ],
        [ rdfs:comment "Human-readable size (e.g., '2.5 GB')." ;
            sh:datatype xsd:string ;
            sh:maxCount 1 ;
            sh:message "Consider specifying version size." ;
            sh:name "size" ;
            sh:path ds:size ;
            sh:severity sh:Info ],
        [ rdfs:comment "Exact size in bytes." ;
            sh:datatype xsd:long ;
            sh:maxCount 1 ;
            sh:message "Size in bytes must be non-negative." ;
            sh:minInclusive 0 ;
            sh:name "sizeBytes" ;
            sh:path ds:sizeBytes ;
            sh:severity sh:Info ],
        [ rdfs:comment "Number of records in this version." ;
            sh:datatype xsd:long ;
            sh:maxCount 1 ;
            sh:message "Record count must be non-negative." ;
            sh:minInclusive 0 ;
            sh:name "recordCount" ;
            sh:path ds:recordCount ;
            sh:severity sh:Info ],
        [ rdfs:comment "URL to download this version." ;
            sh:datatype xsd:anyURI ;
            sh:message "Version should have download URLs." ;
            sh:name "downloadUrl" ;
            sh:path ds:downloadUrl ;
            sh:pattern "^https?://" ;
            sh:severity sh:Warning ] ;
    sh:targetClass ds:Version .

tax:FASTA a ds:FileFormat ;
    ds:description "Sequence format for proteins and nucleic acids" ;
    ds:extension ".faa",
        ".fasta",
        ".fna" ;
    ds:formatName "FASTA" ;
    ds:mimeType "text/x-fasta" .

tax:GenBank a ds:FileFormat ;
    ds:description "GenBank flat file format with annotations" ;
    ds:extension ".gb",
        ".gbff",
        ".gbk" ;
    ds:formatName "GenBank" ;
    ds:mimeType "text/x-genbank" .

tax:PDBFormat a ds:FileFormat ;
    ds:description "Legacy Protein Data Bank format (limited atom count)" ;
    ds:extension ".pdb" ;
    ds:formatName "PDB" ;
    ds:mimeType "chemical/x-pdb" .

tax:QMEAN_Excellent a ds:QualityLevel ;
    ds:confidence "Excellent" ;
    ds:interpretation "High-confidence model" ;
    ds:scoreRange ">-1" .

tax:QMEAN_Good a ds:QualityLevel ;
    ds:confidence "Good" ;
    ds:interpretation "Reliable for most uses" ;
    ds:scoreRange "-1 to -2" .

tax:QMEAN_Moderate a ds:QualityLevel ;
    ds:confidence "Moderate" ;
    ds:interpretation "Use with caution" ;
    ds:scoreRange "-2 to -3" .

tax:QMEAN_Poor a ds:QualityLevel ;
    ds:confidence "Poor" ;
    ds:interpretation "Low reliability" ;
    ds:scoreRange "<-3" .

tax:Resolution_Acceptable a ds:QualityLevel ;
    ds:confidence "Acceptable" ;
    ds:interpretation "Standard resolution structure" ;
    ds:scoreRange "<3.0 A" .

tax:Resolution_High a ds:QualityLevel ;
    ds:confidence "High" ;
    ds:interpretation "High-resolution X-ray structure" ;
    ds:scoreRange "<2.0 A" .

tax:mmCIF a ds:FileFormat ;
    ds:description "Macromolecular CIF - primary PDB format" ;
    ds:extension ".cif" ;
    ds:formatName "mmCIF" ;
    ds:mimeType "chemical/x-mmcif" .

tax:pLDDT_Confident a ds:QualityLevel ;
    ds:confidence "Confident" ;
    ds:interpretation "Generally good; backbone reliable" ;
    ds:scoreRange "70-90" .

tax:pLDDT_Low a ds:QualityLevel ;
    ds:confidence "Low" ;
    ds:interpretation "Structure uncertain; use with caution" ;
    ds:scoreRange "50-70" .

tax:pLDDT_VeryHigh a ds:QualityLevel ;
    ds:confidence "Very high" ;
    ds:interpretation "Highly reliable; suitable for all applications" ;
    ds:scoreRange ">90" .

tax:pLDDT_VeryLow a ds:QualityLevel ;
    ds:confidence "Very low" ;
    ds:interpretation "Likely disordered or unstructured" ;
    ds:scoreRange "<50" .

tax:status-deprecated a skos:Concept ;
    skos:inScheme tax:status-scheme ;
    skos:prefLabel "Deprecated"@en .

tax:status-draft a skos:Concept ;
    skos:inScheme tax:status-scheme ;
    skos:prefLabel "Draft"@en .

version1:agora2 a ds:Version ;
    rdfs:comment "7,206 gut microbe metabolic reconstructions." ;
    ds:belongsToSource data1:vmh ;
    ds:size "5 GB (all models)" ;
    ds:versionDate "2023"^^xsd:gYear ;
    ds:versionName "AGORA2" .

version1:gmrepo-v3 a ds:Version ;
    rdfs:comment "Current version with 118,965 samples." ;
    ds:belongsToSource data1:gmrepo ;
    ds:versionDate "2023"^^xsd:gYear ;
    ds:versionName "GMrepo v3" .

version1:hmp1 a ds:Version ;
    rdfs:comment "Original healthy cohort characterization." ;
    ds:belongsToSource data1:hmp-bodysite,
        data1:hmp-gut ;
    ds:versionName "HMP Phase 1" .

