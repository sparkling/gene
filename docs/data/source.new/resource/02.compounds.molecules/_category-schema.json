      "unified_fields": [
        {
          "name": "compound_id",
          "type": "string",
          "cardinality": "1:1",
          "description": "Primary identifier for natural product compound",
          "sources": ["COCONUT", "NPASS", "NPAtlas"],
          "semantic_definition": "Unique identifier assigned by each database (e.g., CNP0123456, NPC12345, NPA012345)",
          "examples": ["CNP0123456", "NPC12345", "NPA012345"]
        },
        {
          "name": "name",
          "type": "string",
          "cardinality": "1:1",
          "description": "Common or trivial name of the natural product",
          "sources": ["COCONUT", "Dr. Duke's", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Human-readable name for the compound",
          "examples": ["Artemisinin", "Curcumin", "Quercetin"]
        },
        {
          "name": "canonical_smiles",
          "type": "string",
          "cardinality": "1:1",
          "description": "Simplified Molecular Input Line Entry System notation",
          "sources": ["COCONUT", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Standardized SMILES without stereochemistry information",
          "examples": ["CC(C)CCCC(C)C", "CC(=O)Oc1ccccc1C(=O)O"]
        },
        {
          "name": "isomeric_smiles",
          "type": "string",
          "cardinality": "1:1",
          "description": "SMILES with stereochemistry preserved",
          "sources": ["COCONUT", "LOTUS"],
          "semantic_definition": "SMILES notation that includes stereochemical information",
          "examples": ["C[C@@H](O)c1ccccc1"]
        },
        {
          "name": "inchi",
          "type": "string",
          "cardinality": "1:1",
          "description": "IUPAC International Chemical Identifier",
          "sources": ["COCONUT", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Standard structure representation developed by IUPAC",
          "examples": ["InChI=1S/C6H12O6/c7-1-2-3(8)..."]
        },
        {
          "name": "inchi_key",
          "type": "string",
          "cardinality": "1:1",
          "description": "27-character hashed InChI for fast lookups",
          "sources": ["COCONUT", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Fixed-length hash of InChI enabling exact structure matching",
          "examples": ["BSYNRYMUTXBXSQ-UHFFFAOYSA-N"]
        },
        {
          "name": "molecular_formula",
          "type": "string",
          "cardinality": "1:1",
          "description": "Chemical formula showing atom composition",
          "sources": ["COCONUT", "Dr. Duke's", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Elemental composition of the compound",
          "examples": ["C21H30O2", "C9H8O4"]
        },
        {
          "name": "molecular_weight",
          "type": "decimal",
          "cardinality": "1:1",
          "description": "Molecular weight in Daltons",
          "sources": ["COCONUT", "Dr. Duke's", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Average molecular mass considering isotope distribution",
          "examples": [282.35, 180.16]
        },
        {
          "name": "organism_source",
          "type": "object[]",
          "cardinality": "1:N",
          "description": "Biological organisms producing the natural product",
          "sources": ["COCONUT", "Dr. Duke's", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Species or organisms from which compound was isolated",
          "examples": [{"name": "Artemisia annua", "ncbi_taxon_id": 35608}]
        },
        {
          "name": "ncbi_taxon_id",
          "type": "integer",
          "cardinality": "1:1",
          "description": "NCBI Taxonomy identifier for source organism",
          "sources": ["COCONUT", "LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Unique identifier from NCBI Taxonomy database",
          "examples": [35608, 3702]
        },
        {
          "name": "compound_class",
          "type": "string",
          "cardinality": "1:N",
          "description": "Natural product classification",
          "sources": ["COCONUT", "NPASS", "NPAtlas"],
          "semantic_definition": "Chemical or biosynthetic class of the compound",
          "examples": ["Terpenoid", "Alkaloid", "Flavonoid", "Polyketide"]
        },
        {
          "name": "references",
          "type": "object[]",
          "cardinality": "1:N",
          "description": "Literature citations for compound isolation/characterization",
          "sources": ["LOTUS", "NPASS", "NPAtlas"],
          "semantic_definition": "Publications documenting the compound",
          "examples": [{"doi": "10.1016/j.phytochem.2018.01.001", "pmid": 29371045}]
