{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://gene.taxonomy/schemas/uniprot",
  "title": "UniProt Entry Record",
  "description": "JSON Schema for UniProtKB protein entry records",
  "type": "object",
  "properties": {
    "primaryAccession": {
      "type": "string",
      "description": "Primary UniProt accession",
      "pattern": "^[OPQ][0-9][A-Z0-9]{3}[0-9]|[A-NR-Z][0-9]([A-Z][A-Z0-9]{2}[0-9]){1,2}$",
      "examples": ["P04637", "Q9Y6K9"]
    },
    "uniProtkbId": {
      "type": "string",
      "description": "Entry name (mnemonic identifier)",
      "examples": ["P53_HUMAN", "EGFR_HUMAN"]
    },
    "entryType": {
      "type": "string",
      "description": "Entry type indicating review status",
      "enum": ["UniProtKB reviewed (Swiss-Prot)", "UniProtKB unreviewed (TrEMBL)"]
    },
    "proteinExistence": {
      "type": "string",
      "description": "Evidence for protein existence",
      "enum": [
        "1: Evidence at protein level",
        "2: Evidence at transcript level",
        "3: Inferred from homology",
        "4: Predicted",
        "5: Uncertain"
      ]
    },
    "organism": {
      "type": "object",
      "description": "Source organism information",
      "properties": {
        "scientificName": {"type": "string"},
        "commonName": {"type": ["string", "null"]},
        "taxonId": {"type": "integer"},
        "lineage": {
          "type": "array",
          "items": {"type": "string"}
        }
      }
    },
    "proteinDescription": {
      "type": "object",
      "description": "Protein naming information",
      "properties": {
        "recommendedName": {
          "type": "object",
          "properties": {
            "fullName": {"type": "object"},
            "shortNames": {"type": "array"},
            "ecNumbers": {"type": "array"}
          }
        },
        "alternativeNames": {"type": "array"}
      }
    },
    "genes": {
      "type": "array",
      "description": "Gene name information",
      "items": {
        "type": "object",
        "properties": {
          "geneName": {"type": "object"},
          "synonyms": {"type": "array"},
          "orderedLocusNames": {"type": "array"},
          "orfNames": {"type": "array"}
        }
      }
    },
    "sequence": {
      "type": "object",
      "description": "Sequence information",
      "properties": {
        "value": {"type": "string"},
        "length": {"type": "integer"},
        "molWeight": {"type": "integer"},
        "crc64": {"type": "string"},
        "md5": {"type": "string"}
      }
    },
    "features": {
      "type": "array",
      "description": "Sequence features",
      "items": {
        "type": "object",
        "properties": {
          "type": {"type": "string"},
          "location": {
            "type": "object",
            "properties": {
              "start": {"type": "object"},
              "end": {"type": "object"}
            }
          },
          "description": {"type": ["string", "null"]},
          "featureId": {"type": ["string", "null"]},
          "evidences": {"type": "array"}
        }
      }
    },
    "comments": {
      "type": "array",
      "description": "Functional annotation comments",
      "items": {
        "type": "object",
        "properties": {
          "commentType": {"type": "string"},
          "texts": {"type": "array"}
        }
      }
    },
    "uniProtKBCrossReferences": {
      "type": "array",
      "description": "Cross-references to external databases",
      "items": {
        "type": "object",
        "properties": {
          "database": {"type": "string"},
          "id": {"type": "string"},
          "properties": {"type": "array"}
        }
      }
    },
    "keywords": {
      "type": "array",
      "description": "UniProt controlled vocabulary keywords",
      "items": {
        "type": "object",
        "properties": {
          "id": {"type": "string"},
          "name": {"type": "string"}
        }
      }
    }
  },
  "required": ["primaryAccession", "uniProtkbId", "organism", "sequence"],
  "additionalProperties": true
}
