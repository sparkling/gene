# -----------------------------------------------------------------------------
# GMrepo - Gut Microbiota Repository
# -----------------------------------------------------------------------------

data:gmrepo a ds:DataSource ;
    ds:id "gmrepo" ;
    ds:title "GMrepo - Gut Microbiota Repository" ;
    ds:belongsToCategory cat:Microbiome ;
    ds:belongsToSubcategory subcat:GutMicrobiome ;
    ds:tier tax:Tier2 ;
    ds:status tax:Active ;
    ds:lastUpdated "2023"^^xsd:gYear ;
    ds:tag "gut", "microbiome", "metagenomics", "curated", "phenotypes" ;
    ds:maintainerEmail "weihuachen@hust.edu.cn" ;
    ds:maintainedBy maint:HUST ;
    ds:website <https://gmrepo.humangut.info> ;
    ds:updateFrequency "quarterly" ;
    ds:hasLicense license:CC-BY-NC-3.0 ;
    ds:hasAccessMethod access:gmrepo-api, access:gmrepo-web, access:gmrepo-download ;
    ds:hasIdentifier ident:gmrepo-run-accession, ident:gmrepo-project-accession, ident:gmrepo-ncbi-taxon, ident:gmrepo-phenotype ;
    ds:hasUseCase usecase:gmrepo-composition, usecase:gmrepo-phenotype-assoc, usecase:gmrepo-meta-analysis, usecase:gmrepo-reference, usecase:gmrepo-biomarker ;
    ds:hasLimitation limitation:gmrepo-commercial ;
    rdfs:comment "GMrepo is a manually curated database of human gut metagenomes. It integrates data from multiple sources with standardized phenotype metadata, taxonomic profiles, and quality metrics." ;
    rdfs:comment "Source documentation: 09.microbiome/9.1.gut.microbiome/gmrepo" .

data:gmrepo ds:recordCount [
    ds:metricName "samples" ;
    ds:metricValue "70000"^^xsd:integer
] .

data:gmrepo ds:recordCount [
    ds:metricName "projects" ;
    ds:metricValue "400"^^xsd:integer
] .

data:gmrepo ds:recordCount [
    ds:metricName "species" ;
    ds:metricValue "7000"^^xsd:integer
] .

data:gmrepo ds:recordCount [
    ds:metricName "phenotypes" ;
    ds:metricValue "100"^^xsd:integer
] .

access:gmrepo-api a ds:AccessMethod ;
    ds:accessType "REST API" ;
    ds:accessUrl <https://gmrepo.humangut.info/api> ;
    ds:rateLimit "1 request per second" ;
    ds:format "JSON" ;
    rdfs:comment "Programmatic access to sample, project, and abundance data." .

access:gmrepo-web a ds:AccessMethod ;
    ds:accessType "Web Interface" ;
    ds:accessUrl <https://gmrepo.humangut.info> ;
    ds:format "HTML" ;
    rdfs:comment "Search and browse interface with data export." .

access:gmrepo-download a ds:AccessMethod ;
    ds:accessType "Bulk Download" ;
    ds:accessUrl <https://gmrepo.humangut.info/Downloads> ;
    ds:format "TSV", "JSON" ;
    rdfs:comment "Bulk data downloads including abundance tables." .

ident:gmrepo-run-accession a ds:Identifier ;
    ds:identifierName "Run Accession" ;
    ds:identifierPattern "[SED]RR[0-9]+" ;
    ds:identifierExample "SRR1234567" ;
    rdfs:comment "SRA/ENA sequencing run accession." .

ident:gmrepo-project-accession a ds:Identifier ;
    ds:identifierName "Project Accession" ;
    ds:identifierPattern "PRJNA[0-9]+" ;
    ds:identifierExample "PRJNA123456" ;
    rdfs:comment "NCBI BioProject accession." .

ident:gmrepo-ncbi-taxon a ds:Identifier ;
    ds:identifierName "NCBI Taxon ID" ;
    ds:identifierPattern "[0-9]+" ;
    ds:identifierExample "562" ;
    rdfs:comment "NCBI Taxonomy identifier for species." .

ident:gmrepo-phenotype a ds:Identifier ;
    ds:identifierName "Phenotype" ;
    ds:identifierPattern "Controlled vocabulary" ;
    ds:identifierExample "Healthy, IBD, T2D" ;
    rdfs:comment "Standardized health condition or phenotype." .

usecase:gmrepo-composition a ds:UseCase ;
    ds:useCaseName "Gut microbiome composition analysis" ;
    rdfs:comment "Analyze species abundances and community structure." .

usecase:gmrepo-phenotype-assoc a ds:UseCase ;
    ds:useCaseName "Microbiome-phenotype associations" ;
    rdfs:comment "Discover associations between microbiome and health conditions." .

usecase:gmrepo-meta-analysis a ds:UseCase ;
    ds:useCaseName "Cross-study meta-analysis" ;
    rdfs:comment "Compare results across multiple studies with standardized metadata." .

usecase:gmrepo-reference a ds:UseCase ;
    ds:useCaseName "Reference taxonomic profiles" ;
    rdfs:comment "Use as reference profiles for new microbiome samples." .

usecase:gmrepo-biomarker a ds:UseCase ;
    ds:useCaseName "Biomarker discovery" ;
    rdfs:comment "Identify microbial biomarkers for diseases." .

limitation:gmrepo-commercial a ds:Limitation ;
    ds:limitationName "Commercial use restriction" ;
    ds:limitationDescription "Commercial use requires permission from maintainers." ;
    ds:impact "high" .

