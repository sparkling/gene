# -----------------------------------------------------------------------------
# COCONUT - COlleCtion of Open Natural prodUcTs
# -----------------------------------------------------------------------------

data:coconut a ds:DataSource ;
    ds:id "coconut" ;
    ds:title "COCONUT - COlleCtion of Open Natural prodUcTs"@en ;
    ds:description "One of the largest open databases of natural products, aggregating compound data from over 50 source databases with comprehensive structural, physicochemical, and biological information."@en ;
    ds:tier tax:Tier1 ;
    ds:status tax:Active ;
    ds:belongsToCategory cat:CompoundsMolecules ;
    ds:belongsToSubcategory tax:subcategory-natural-products ;
    ds:maintainedBy maint:SteinbeckLab ;
    ds:website <https://coconut.naturalproducts.net> ;
    ds:updateFrequency "quarterly" ;
    ds:recordCount 450000 ;
    ds:hasLicense data:coconut-license ;
    ds:hasAccessMethod data:coconut-api, data:coconut-download ;
    ds:hasIdentifier data:coconut-id-coconut, data:coconut-id-inchikey ;
    ds:hasUseCase data:coconut-usecase-1, data:coconut-usecase-2, data:coconut-usecase-3, data:coconut-usecase-4, data:coconut-usecase-5 ;
    ds:hasLimitation data:coconut-limitation-1 .

data:coconut-license a ds:License ;
    ds:licenseName "Creative Commons Attribution 4.0"@en ;
    ds:spdxId "CC-BY-4.0" ;
    ds:commercialUse true ;
    ds:attributionRequired true ;
    ds:redistribution true .

data:coconut-api a ds:AccessMethod ;
    ds:methodType tax:access-rest-api ;
    ds:url <https://coconut.naturalproducts.net/api> ;
    ds:format tax:format-json .

data:coconut-download a ds:AccessMethod ;
    ds:methodType tax:access-bulk-download ;
    ds:url <https://coconut.naturalproducts.net/download> ;
    ds:format tax:format-sdf, tax:format-csv, tax:format-json .

data:coconut-id-coconut a ds:Identifier ;
    ds:identifierType "COCONUT ID" ;
    ds:pattern "CNP + 7 digits" ;
    ds:example "CNP0123456" .

data:coconut-id-inchikey a ds:Identifier ;
    ds:identifierType "InChI Key" ;
    ds:pattern "27 characters" ;
    ds:example "BSYNRYMUTXBXSQ-UHFFFAOYSA-N" .

data:coconut-usecase-1 a ds:UseCase ;
    ds:description "Natural Product Drug Discovery - Identify bioactive scaffolds and lead compounds from natural sources"@en .

data:coconut-usecase-2 a ds:UseCase ;
    ds:description "Structure-Activity Relationship Studies - Explore chemical diversity and scaffold analysis"@en .

data:coconut-usecase-3 a ds:UseCase ;
    ds:description "Compound Dereplication - Check novelty of isolated natural products against known compounds"@en .

data:coconut-usecase-4 a ds:UseCase ;
    ds:description "Organism-Compound Mapping - Link natural products to their biological source species"@en .

data:coconut-usecase-5 a ds:UseCase ;
    ds:description "Drug-Likeness Screening - Filter compounds by physicochemical properties"@en .

data:coconut-limitation-1 a ds:Limitation ;
    ds:description "Aggregated data quality varies across source databases; some entries may lack complete structural validation"@en .

