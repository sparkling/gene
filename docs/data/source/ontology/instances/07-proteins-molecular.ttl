    ds:belongsToCategory cat:ProteinsMolecularBiology ;
    ds:belongsToSubcategory subcat:ProteinStructures ;
    ds:tier tax:Tier2 ;
    ds:status tax:Active ;
    ds:maintainedBy maint:SIB ;
    ds:website "https://swissmodel.expasy.org" ;
    ds:updateFrequency "Continuous with monthly repository refresh" ;
    ds:lastUpdated "2026-01-23"^^xsd:date ;
    ds:tag "modeling" ;
    ds:tag "homology" ;
    ds:tag "templates" ;
    ds:tag "3d" ;
    ds:tag "prediction" ;
    ds:hasLicense license:swiss-model-license ;
    ds:hasAccessMethod access:swiss-model-web ;
    ds:hasAccessMethod access:swiss-model-repository ;
    ds:hasAccessMethod access:swiss-model-api ;
    ds:hasIdentifier id:swiss-model-id ;
    ds:hasIdentifier id:swiss-model-uniprot ;
    ds:hasIdentifier id:swiss-model-template ;
    ds:hasIdentifier id:swiss-model-qmean ;
    ds:hasUseCase usecase:swiss-model-homology ;
    ds:hasUseCase usecase:swiss-model-template-based ;
    ds:hasUseCase usecase:swiss-model-quality ;
    ds:hasUseCase usecase:swiss-model-comparative ;
    ds:hasUseCase usecase:swiss-model-structure-function ;
    ds:hasLimitation limitation:swiss-model-template-dependent ;
    ds:hasLimitation limitation:swiss-model-coverage ;
    ds:hasLimitation limitation:swiss-model-rate-limit ;
