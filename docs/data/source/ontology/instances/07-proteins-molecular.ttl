    ds:useCaseTitle "Template-based structure prediction" ;
    ds:useCaseDescription "Predict protein structure using known homologous structures" .

usecase:swiss-model-quality a ds:UseCase ;
    ds:useCaseTitle "Model quality assessment" ;
    ds:useCaseDescription "Assess model reliability using QMEAN and QMEANDisCo scores" .

usecase:swiss-model-comparative a ds:UseCase ;
    ds:useCaseTitle "Comparative structural analysis" ;
    ds:useCaseDescription "Compare structures across protein families and orthologs" .

usecase:swiss-model-structure-function a ds:UseCase ;
    ds:useCaseTitle "Structure-function relationship studies" ;
    ds:useCaseDescription "Understand protein function through structural context" .

# SWISS-MODEL Limitations
limitation:swiss-model-template-dependent a ds:Limitation ;
    ds:limitationDescription "Model quality depends on template sequence identity; minimum 30% recommended" ;
    ds:impact tax:High .

limitation:swiss-model-coverage a ds:Limitation ;
