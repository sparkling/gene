# =============================================================================
# QUALITY SCORE ENUMERATIONS
# =============================================================================

# pLDDT Score Ranges (AlphaFold)
tax:pLDDT_VeryHigh a ds:QualityLevel ;
    ds:scoreRange ">90" ;
    ds:confidence "Very high" ;
    ds:interpretation "Highly reliable; suitable for all applications" .

tax:pLDDT_Confident a ds:QualityLevel ;
    ds:scoreRange "70-90" ;
    ds:confidence "Confident" ;
    ds:interpretation "Generally good; backbone reliable" .

tax:pLDDT_Low a ds:QualityLevel ;
    ds:scoreRange "50-70" ;
    ds:confidence "Low" ;
    ds:interpretation "Structure uncertain; use with caution" .

tax:pLDDT_VeryLow a ds:QualityLevel ;
    ds:scoreRange "<50" ;
    ds:confidence "Very low" ;
    ds:interpretation "Likely disordered or unstructured" .

# QMEAN Score Ranges (SWISS-MODEL)
tax:QMEAN_Excellent a ds:QualityLevel ;
    ds:scoreRange ">-1" ;
    ds:confidence "Excellent" ;
    ds:interpretation "High-confidence model" .

tax:QMEAN_Good a ds:QualityLevel ;
    ds:scoreRange "-1 to -2" ;
    ds:confidence "Good" ;
    ds:interpretation "Reliable for most uses" .

tax:QMEAN_Moderate a ds:QualityLevel ;
    ds:scoreRange "-2 to -3" ;
    ds:confidence "Moderate" ;
    ds:interpretation "Use with caution" .

tax:QMEAN_Poor a ds:QualityLevel ;
    ds:scoreRange "<-3" ;
    ds:confidence "Poor" ;
    ds:interpretation "Low reliability" .

# Experimental Quality (PDB)
tax:Resolution_High a ds:QualityLevel ;
    ds:scoreRange "<2.0 A" ;
    ds:confidence "High" ;
    ds:interpretation "High-resolution X-ray structure" .

tax:Resolution_Acceptable a ds:QualityLevel ;
    ds:scoreRange "<3.0 A" ;
    ds:confidence "Acceptable" ;
    ds:interpretation "Standard resolution structure" .

