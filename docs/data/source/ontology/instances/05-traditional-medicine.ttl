    ds:accessType tax:SPARQLEndpoint ;
    ds:baseURL "https://query.wikidata.org/sparql" ;
    ds:authentication false ;
    ds:responseFormat "JSON, XML, CSV, TSV" .

access:wikidata-api a ds:AccessMethod ;
    ds:forDataSource data:wikidata ;
    ds:accessType tax:RESTAPI ;
    ds:baseURL "https://www.wikidata.org/w/api.php" ;
    ds:authentication false ;
    ds:responseFormat "JSON" .

access:wikidata-dump a ds:AccessMethod ;
    ds:forDataSource data:wikidata ;
    ds:accessType tax:BulkDownload ;
    ds:baseURL "https://dumps.wikimedia.org/wikidatawiki/" ;
    ds:downloadFormat "JSON, RDF" .

identifier:wikidata-qid a ds:Identifier ;
    ds:forDataSource data:wikidata ;
    ds:identifierName "QID (Item)" ;
    ds:pattern "Q[0-9]+" ;
    ds:example "Q21173022" .

identifier:wikidata-pid a ds:Identifier ;
    ds:forDataSource data:wikidata ;
    ds:identifierName "PID (Property)" ;
    ds:pattern "P[0-9]+" ;
    ds:example "P352" .

usecase:wikidata-resolution a ds:UseCase ;
    ds:forDataSource data:wikidata ;
    ds:useCaseDescription "Entity resolution and linking across databases" .

usecase:wikidata-kg a ds:UseCase ;
    ds:forDataSource data:wikidata ;
    ds:useCaseDescription "Knowledge graph construction and semantic web applications" .

limitation:wikidata-quality a ds:Limitation ;
    ds:forDataSource data:wikidata ;
    ds:limitationDescription "Community-edited content may have quality variations" ;
    ds:impactLevel "medium" .

limitation:wikidata-coverage a ds:Limitation ;
    ds:forDataSource data:wikidata ;
    ds:limitationDescription "Biomedical coverage varies by entity type" ;
    ds:impactLevel "low" .

xref:wikidata-uniprot a ds:CrossReference ;
    ds:forDataSource data:wikidata ;
    ds:targetDatabase "UniProt" ;
    ds:coverageLevel "comprehensive" ;
    ds:linkType "protein identifiers via P352" .

xref:wikidata-pubchem a ds:CrossReference ;
    ds:forDataSource data:wikidata ;
    ds:targetDatabase "PubChem" ;
    ds:coverageLevel "high" ;
    ds:linkType "compound identifiers" .

xref:wikidata-ncbi a ds:CrossReference ;
    ds:forDataSource data:wikidata ;
    ds:targetDatabase "NCBI Gene" ;
    ds:coverageLevel "comprehensive" ;
    ds:linkType "gene identifiers" .

