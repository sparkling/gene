# -----------------------------------------------------------------------------
# ICD - International Classification of Diseases
# -----------------------------------------------------------------------------

data:ICD a ds:DataSource ;
    ds:id "icd" ;
    ds:title "ICD - International Classification of Diseases" ;
    ds:description "WHO-maintained international standard for diagnostic classification of diseases and health problems, used globally for health statistics and billing." ;
    ds:belongsToSubcategory subcat:DiseaseOntologies ;
    ds:tier tax:Tier1 ;
    ds:status tax:Active ;
    ds:maintainedBy maint:WHO ;
    ds:website <https://www.who.int/standards/classifications/classification-of-diseases> ;
    ds:updateFrequency tax:Annual ;
    ds:hasLicense data:ICD_License ;
    ds:hasAccessMethod data:ICD_WebAccess, data:ICD_APIAccess ;
    ds:hasIdentifier data:ICD_ID_ICD10, data:ICD_ID_ICD11 ;
    ds:hasUseCase data:ICD_UseCase_1, data:ICD_UseCase_2 ;
    ds:hasLimitation data:ICD_Limitation_1 .

data:ICD_License a ds:License ;
    ds:licenseName "WHO License" ;
    ds:spdxIdentifier "Custom" ;
    ds:commercialUse true ;
    ds:attributionRequired true ;
    ds:redistributionAllowed true ;
    ds:licenseURL <https://www.who.int/about/policies/publishing/copyright> .

data:ICD_WebAccess a ds:AccessMethod ;
    ds:accessType tax:WebInterface ;
    ds:endpoint <https://icd.who.int/> .

data:ICD_APIAccess a ds:AccessMethod ;
    ds:accessType tax:RESTAPI ;
    ds:endpoint <https://id.who.int/icd/> ;
    ds:authRequired true .

data:ICD_ID_ICD10 a ds:Identifier ;
    ds:pattern "[A-Z][0-9]{2}(\\.[0-9]{1,2})?" ;
    ds:example "E11.9" ;
    ds:description "ICD-10 diagnosis code" .

data:ICD_ID_ICD11 a ds:Identifier ;
    ds:pattern "[0-9A-Z]{4,6}" ;
    ds:example "5A11" ;
    ds:description "ICD-11 diagnosis code" .

data:ICD_UseCase_1 a ds:UseCase ;
    ds:description "Clinical coding for healthcare billing and statistics" .

data:ICD_UseCase_2 a ds:UseCase ;
    ds:description "Epidemiological surveillance and health reporting" .

data:ICD_Limitation_1 a ds:Limitation ;
    ds:description "Granularity insufficient for detailed clinical phenotyping" ;
    ds:impact tax:Medium .

