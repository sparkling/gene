# =============================================================================
# AUXILIARY CONCEPT SCHEMES
# =============================================================================

# -----------------------------------------------------------------------------
# Tier Classification Scheme
# -----------------------------------------------------------------------------

tax:TierScheme a skos:ConceptScheme ;
    skos:prefLabel "Data Source Tier Classification" ;
    rdfs:comment "Quality and priority tiers for data sources based on coverage, reliability, and utility." ;
    skos:hasTopConcept tax:Tier1 , tax:Tier2 , tax:Tier3 .

tax:Tier1 a skos:Concept ;
    skos:prefLabel "Tier 1 - Primary" ;
    skos:altLabel "1" ;
    skos:notation "1" ;
    rdfs:comment "Primary authoritative sources with comprehensive coverage, high reliability, and frequent updates. Essential for most use cases." ;
    skos:inScheme tax:TierScheme ;
    skos:topConceptOf tax:TierScheme .

tax:Tier2 a skos:Concept ;
    skos:prefLabel "Tier 2 - Secondary" ;
    skos:altLabel "2" ;
    skos:notation "2" ;
    rdfs:comment "Valuable supplementary sources with good coverage in specific domains. Useful for enrichment and validation." ;
    skos:inScheme tax:TierScheme ;
    skos:topConceptOf tax:TierScheme .

tax:Tier3 a skos:Concept ;
    skos:prefLabel "Tier 3 - Specialized" ;
    skos:altLabel "3" ;
    skos:notation "3" ;
    rdfs:comment "Specialized or niche sources with limited scope but unique data. Useful for specific research questions." ;
    skos:inScheme tax:TierScheme ;
    skos:topConceptOf tax:TierScheme .

# -----------------------------------------------------------------------------
# Status Scheme
# -----------------------------------------------------------------------------

tax:StatusScheme a skos:ConceptScheme ;
    skos:prefLabel "Data Source Status" ;
    rdfs:comment "Lifecycle status indicators for data source documentation." ;
    skos:hasTopConcept tax:Draft , tax:Active , tax:Deprecated .

tax:Draft a skos:Concept ;
    skos:prefLabel "Draft" ;
    skos:notation "draft" ;
    rdfs:comment "Documentation is incomplete or under review. Data may not be fully validated." ;
    skos:inScheme tax:StatusScheme ;
    skos:topConceptOf tax:StatusScheme .

tax:Active a skos:Concept ;
    skos:prefLabel "Active" ;
    skos:notation "active" ;
    rdfs:comment "Documentation is complete and validated. Data source is recommended for use." ;
    skos:inScheme tax:StatusScheme ;
    skos:topConceptOf tax:StatusScheme .

tax:Deprecated a skos:Concept ;
    skos:prefLabel "Deprecated" ;
    skos:notation "deprecated" ;
    rdfs:comment "Data source is no longer recommended. May be superseded or discontinued." ;
    skos:inScheme tax:StatusScheme ;
    skos:topConceptOf tax:StatusScheme .

# -----------------------------------------------------------------------------
# Access Method Scheme
# -----------------------------------------------------------------------------

tax:AccessMethodScheme a skos:ConceptScheme ;
    skos:prefLabel "Data Access Methods" ;
    rdfs:comment "Available methods for accessing data from sources." ;
    skos:hasTopConcept
        tax:RestApi ,
        tax:SparqlEndpoint ,
        tax:GraphqlApi ,
        tax:FtpDownload ,
        tax:S3Download ,
        tax:RsyncDownload ,
        tax:DirectDownload ,
        tax:WebScraping ,
        tax:RESTAPI ,
        tax:SPARQL ,
        tax:FTP ,
        tax:WebInterface ,
        tax:GitHubRepository ,
        tax:GraphQL ,
        tax:CLI ,
        tax:CloudStorage ,
        tax:CytoscapeApp ,
        tax:DockerImage ,
        tax:JupyterNotebook ,
        tax:RPackage ,
        tax:PyPIPackage ,
        tax:BioconductorPackage ,
