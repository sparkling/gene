# -----------------------------------------------------------------------------
# Quality Indicator Scheme
# -----------------------------------------------------------------------------

tax:QualityIndicatorScheme a skos:ConceptScheme ;
    skos:prefLabel "Quality Indicators" ;
    rdfs:comment "Quality metrics and confidence indicators used by data sources." ;
    skos:hasTopConcept
        tax:pLDDT ,
        tax:QMEAN ,
        tax:Coverage ,
        tax:Resolution ,
        tax:HighConfidence ,
        tax:MediumConfidence ,
        tax:LowConfidence .

tax:pLDDT a skos:Concept ;
    skos:prefLabel "pLDDT" ;
    skos:altLabel "predicted Local Distance Difference Test" ;
    skos:notation "plddt" ;
    rdfs:comment "AlphaFold confidence score (0-100) indicating predicted local structure accuracy. Score >90 indicates high confidence, 70-90 moderate, <70 low." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:topConceptOf tax:QualityIndicatorScheme .

tax:QMEAN a skos:Concept ;
    skos:prefLabel "QMEAN" ;
    skos:altLabel "Qualitative Model Energy Analysis" ;
    skos:notation "qmean" ;
    rdfs:comment "Global and local quality estimates for protein structure models. Score range typically -4 to 0, where 0 is best." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:topConceptOf tax:QualityIndicatorScheme .

tax:Coverage a skos:Concept ;
    skos:prefLabel "Coverage" ;
    skos:altLabel "Sequence Coverage" , "Genome Coverage" ;
    skos:notation "coverage" ;
    rdfs:comment "Percentage or fraction of target sequence/genome that is represented or aligned in the data." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:topConceptOf tax:QualityIndicatorScheme .

# -----------------------------------------------------------------------------
# pLDDT Quality Level Concepts
# -----------------------------------------------------------------------------

tax:pLDDT_VeryHigh a skos:Concept ;
    skos:prefLabel "pLDDT Very High" ;
    skos:altLabel "pLDDT >90" ;
    skos:notation "plddt-very-high" ;
    skos:definition "AlphaFold pLDDT score greater than 90, indicating very high confidence in predicted local structure accuracy." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:pLDDT .

tax:pLDDT_High a skos:Concept ;
    skos:prefLabel "pLDDT High" ;
    skos:altLabel "pLDDT 70-90" ;
    skos:notation "plddt-high" ;
    skos:definition "AlphaFold pLDDT score between 70 and 90, indicating high confidence in predicted local structure accuracy." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:pLDDT .

tax:pLDDT_Medium a skos:Concept ;
    skos:prefLabel "pLDDT Medium" ;
    skos:altLabel "pLDDT 50-70" ;
    skos:notation "plddt-medium" ;
    skos:definition "AlphaFold pLDDT score between 50 and 70, indicating moderate confidence in predicted local structure accuracy." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:pLDDT .

tax:pLDDT_Low a skos:Concept ;
    skos:prefLabel "pLDDT Low" ;
    skos:altLabel "pLDDT <50" ;
    skos:notation "plddt-low" ;
    skos:definition "AlphaFold pLDDT score less than 50, indicating low confidence in predicted local structure accuracy. Structure may be disordered or poorly predicted." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:pLDDT .

tax:pLDDT_Confident a skos:Concept ;
    skos:prefLabel "pLDDT Confident" ;
    skos:altLabel "pLDDT 70-90" ;
    skos:notation "plddt-confident" ;
    skos:definition "AlphaFold pLDDT score between 70 and 90, indicating confident prediction suitable for most structural analyses." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:pLDDT ;
    skos:closeMatch tax:pLDDT_High .

tax:pLDDT_VeryLow a skos:Concept ;
    skos:prefLabel "pLDDT Very Low" ;
    skos:altLabel "pLDDT <30" ;
    skos:notation "plddt-very-low" ;
    skos:definition "AlphaFold pLDDT score less than 30, indicating very low confidence. Structure is likely disordered, flexible, or poorly predicted." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:pLDDT .

# -----------------------------------------------------------------------------
# QMEAN Quality Level Concepts
# -----------------------------------------------------------------------------

tax:QMEAN_Good a skos:Concept ;
    skos:prefLabel "QMEAN Good" ;
    skos:altLabel "QMEAN >0.7" ;
    skos:notation "qmean-good" ;
    skos:definition "QMEAN score greater than 0.7 (or Z-score near 0), indicating good model quality comparable to experimental structures." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:QMEAN .

tax:QMEAN_Medium a skos:Concept ;
    skos:prefLabel "QMEAN Medium" ;
    skos:altLabel "QMEAN 0.5-0.7" ;
    skos:notation "qmean-medium" ;
    skos:definition "QMEAN score between 0.5 and 0.7, indicating acceptable model quality suitable for many applications." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:QMEAN .

tax:QMEAN_Low a skos:Concept ;
    skos:prefLabel "QMEAN Low" ;
    skos:altLabel "QMEAN <0.5" ;
    skos:notation "qmean-low" ;
    skos:definition "QMEAN score less than 0.5, indicating low model quality. Use with caution for structural analysis." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:QMEAN .

tax:QMEAN_Excellent a skos:Concept ;
    skos:prefLabel "QMEAN Excellent" ;
    skos:altLabel "QMEAN >0.8" ;
    skos:notation "qmean-excellent" ;
    skos:definition "QMEAN score greater than 0.8, indicating excellent model quality comparable to high-resolution experimental structures." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:QMEAN .

tax:QMEAN_Moderate a skos:Concept ;
    skos:prefLabel "QMEAN Moderate" ;
    skos:altLabel "QMEAN 0.5-0.6" ;
    skos:notation "qmean-moderate" ;
    skos:definition "QMEAN score between 0.5 and 0.6, indicating moderate model quality suitable for general fold analysis." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:QMEAN .

tax:QMEAN_Poor a skos:Concept ;
    skos:prefLabel "QMEAN Poor" ;
    skos:altLabel "QMEAN <0.4" ;
    skos:notation "qmean-poor" ;
    skos:definition "QMEAN score less than 0.4, indicating poor model quality. Structure should be interpreted with significant caution." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:QMEAN .

# -----------------------------------------------------------------------------
# Resolution Quality Level Concepts
# -----------------------------------------------------------------------------

tax:Resolution a skos:Concept ;
    skos:prefLabel "Resolution" ;
    skos:altLabel "Crystallographic Resolution" , "Structural Resolution" ;
    skos:notation "resolution" ;
    skos:definition "Crystallographic or cryo-EM resolution in Angstroms, indicating the level of structural detail visible in experimental structures." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:topConceptOf tax:QualityIndicatorScheme .

tax:Resolution_High a skos:Concept ;
    skos:prefLabel "Resolution High" ;
    skos:altLabel "Resolution <2.0A" ;
    skos:notation "resolution-high" ;
    skos:definition "Structural resolution less than 2.0 Angstroms, providing atomic-level detail suitable for drug design and detailed mechanistic studies." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Resolution .

tax:Resolution_Medium a skos:Concept ;
    skos:prefLabel "Resolution Medium" ;
    skos:altLabel "Resolution 2.0-3.0A" ;
    skos:notation "resolution-medium" ;
    skos:definition "Structural resolution between 2.0 and 3.0 Angstroms, sufficient for most structural biology applications." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Resolution .

tax:Resolution_Low a skos:Concept ;
    skos:prefLabel "Resolution Low" ;
    skos:altLabel "Resolution >3.0A" ;
    skos:notation "resolution-low" ;
    skos:definition "Structural resolution greater than 3.0 Angstroms. Limited detail; suitable for overall fold and domain architecture." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Resolution .

tax:Resolution_Acceptable a skos:Concept ;
    skos:prefLabel "Resolution Acceptable" ;
    skos:altLabel "Resolution 2.5-3.5A" ;
    skos:notation "resolution-acceptable" ;
    skos:definition "Structural resolution between 2.5 and 3.5 Angstroms, providing acceptable detail for general structural analysis and domain identification." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Resolution .

# -----------------------------------------------------------------------------
# Confidence Level Concepts
# -----------------------------------------------------------------------------

tax:HighConfidence a skos:Concept ;
    skos:prefLabel "High Confidence" ;
    skos:altLabel "High Quality" , "Confident" ;
    skos:notation "high-confidence" ;
    skos:definition "Data or prediction with high confidence level, typically supported by multiple lines of evidence or validated experimentally." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:topConceptOf tax:QualityIndicatorScheme .

tax:MediumConfidence a skos:Concept ;
    skos:prefLabel "Medium Confidence" ;
    skos:altLabel "Moderate Confidence" , "Probable" ;
    skos:notation "medium-confidence" ;
    skos:definition "Data or prediction with moderate confidence level, typically supported by computational evidence or limited experimental validation." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:topConceptOf tax:QualityIndicatorScheme .

tax:LowConfidence a skos:Concept ;
    skos:prefLabel "Low Confidence" ;
    skos:altLabel "Uncertain" , "Predicted" ;
    skos:notation "low-confidence" ;
    skos:definition "Data or prediction with low confidence level, typically based on computational prediction alone or conflicting evidence." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:topConceptOf tax:QualityIndicatorScheme .

# -----------------------------------------------------------------------------
# Coverage Level Concepts
# -----------------------------------------------------------------------------

tax:Coverage_Complete a skos:Concept ;
    skos:prefLabel "Coverage Complete" ;
    skos:altLabel "Full Coverage" , "100% Coverage" ;
    skos:notation "coverage-complete" ;
    skos:definition "Complete or full coverage of the target sequence, genome, or dataset with no significant gaps." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Coverage .

tax:Coverage_High a skos:Concept ;
    skos:prefLabel "Coverage High" ;
    skos:altLabel "High Coverage" , ">80% Coverage" ;
    skos:notation "coverage-high" ;
    skos:definition "High coverage level, typically greater than 80% of the target sequence or dataset." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Coverage .

tax:Coverage_Medium a skos:Concept ;
    skos:prefLabel "Coverage Medium" ;
    skos:altLabel "Moderate Coverage" , "50-80% Coverage" ;
    skos:notation "coverage-medium" ;
    skos:definition "Medium coverage level, typically between 50% and 80% of the target sequence or dataset." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Coverage .

tax:Coverage_Low a skos:Concept ;
    skos:prefLabel "Coverage Low" ;
    skos:altLabel "Low Coverage" , "<50% Coverage" ;
    skos:notation "coverage-low" ;
    skos:definition "Low coverage level, typically less than 50% of the target sequence or dataset." ;
    skos:inScheme tax:QualityIndicatorScheme ;
    skos:broader tax:Coverage .

# =============================================================================
# CATEGORY FACET SCHEME
# =============================================================================
# Faceted classification concepts for categorizing data sources by domain,
# access characteristics, and structural organization.
# =============================================================================

tax:CategoryFacetScheme a skos:ConceptScheme ;
    skos:prefLabel "Category Facets" ;
    rdfs:comment "Faceted classification concepts for organizing data sources by research domain, access type, and structural hierarchy. Used as classification tags." ;
    dct:created "2026-01-26"^^xsd:date ;
    skos:hasTopConcept
        tax:access ,
        tax:cancer ,
        tax:expression ,
        tax:format ,
        tax:functional ,
        tax:pharmacogenomics_facet ,
        tax:population ,
        tax:status ,
        tax:subcategory .

# -----------------------------------------------------------------------------
# Access Category Facet
# -----------------------------------------------------------------------------

tax:access a skos:Concept ;
    skos:prefLabel "Access" ;
    skos:altLabel "Access Category" , "Access Type" ;
    skos:notation "access" ;
    skos:definition "Facet for categorizing data sources by their access characteristics and methods. Links to AccessMethodScheme for specific access types." ;
    skos:inScheme tax:CategoryFacetScheme ;
    skos:topConceptOf tax:CategoryFacetScheme ;
    skos:related tax:AccessMethodScheme .

# -----------------------------------------------------------------------------
# Cancer Research Domain Facet
# -----------------------------------------------------------------------------

