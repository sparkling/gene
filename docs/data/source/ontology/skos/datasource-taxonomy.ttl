# -----------------------------------------------------------------------------
# License Type Scheme
# -----------------------------------------------------------------------------

tax:LicenseTypeScheme a skos:ConceptScheme ;
    skos:prefLabel "License Types" ;
    rdfs:comment "Common license types for data sources." ;
    skos:hasTopConcept
        tax:PublicDomain ,
        tax:OpenAccess ,
        tax:CreativeCommons ,
        tax:AcademicOnly ,
        tax:CommercialRestricted ,
        tax:Proprietary .

tax:PublicDomain a skos:Concept ;
    skos:prefLabel "Public Domain" ;
    skos:altLabel "CC0" , "No Copyright" ;
    skos:notation "public-domain" ;
    rdfs:comment "No restrictions on use. Data is in the public domain." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:topConceptOf tax:LicenseTypeScheme .

tax:OpenAccess a skos:Concept ;
    skos:prefLabel "Open Access" ;
    skos:altLabel "Open Data" ;
    skos:notation "open-access" ;
    rdfs:comment "Open access with minimal restrictions, typically attribution required." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:topConceptOf tax:LicenseTypeScheme .

tax:CreativeCommons a skos:Concept ;
    skos:prefLabel "Creative Commons" ;
    skos:altLabel "CC" ;
    skos:notation "cc" ;
    rdfs:comment "Creative Commons licensed with various restriction levels (BY, NC, SA, ND)." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:topConceptOf tax:LicenseTypeScheme ;
    skos:narrower tax:CcBy , tax:CcByNc , tax:CcBySa , tax:CcByNcSa .

tax:CcBy a skos:Concept ;
    skos:prefLabel "CC BY" ;
    skos:notation "cc-by" ;
    rdfs:comment "Creative Commons Attribution - Commercial use allowed with attribution." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:broader tax:CreativeCommons .

tax:CcByNc a skos:Concept ;
    skos:prefLabel "CC BY-NC" ;
    skos:notation "cc-by-nc" ;
    rdfs:comment "Creative Commons Attribution-NonCommercial - Non-commercial use only." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:broader tax:CreativeCommons .

tax:CcBySa a skos:Concept ;
    skos:prefLabel "CC BY-SA" ;
    skos:notation "cc-by-sa" ;
    rdfs:comment "Creative Commons Attribution-ShareAlike - Derivatives must use same license." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:broader tax:CreativeCommons .

tax:CcByNcSa a skos:Concept ;
    skos:prefLabel "CC BY-NC-SA" ;
    skos:notation "cc-by-nc-sa" ;
    rdfs:comment "Creative Commons Attribution-NonCommercial-ShareAlike." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:broader tax:CreativeCommons .

tax:AcademicOnly a skos:Concept ;
    skos:prefLabel "Academic Use Only" ;
    skos:altLabel "Research Only" , "Non-Commercial Research" ;
    skos:notation "academic-only" ;
    rdfs:comment "Restricted to academic and non-commercial research use." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:topConceptOf tax:LicenseTypeScheme .

tax:CommercialRestricted a skos:Concept ;
    skos:prefLabel "Commercial Restricted" ;
    skos:notation "commercial-restricted" ;
    rdfs:comment "Commercial use requires separate license agreement." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:topConceptOf tax:LicenseTypeScheme .

tax:Proprietary a skos:Concept ;
    skos:prefLabel "Proprietary" ;
    skos:altLabel "Subscription" , "Licensed" ;
    skos:notation "proprietary" ;
    rdfs:comment "Proprietary data requiring subscription or license agreement." ;
    skos:inScheme tax:LicenseTypeScheme ;
    skos:topConceptOf tax:LicenseTypeScheme .

