    skos:inScheme tax:DataSourceTaxonomy ;
    skos:topConceptOf tax:DataSourceTaxonomy ;
    skos:narrower
        tax:NaturalProducts ,
        tax:Pharmaceuticals ,
        tax:TraditionalMedicineCompounds ,
        tax:FoodCompoundsNutrients ,
        tax:DrugMetabolism ,
        tax:ChemicalOntology ,
        tax:CompoundTargetInteractions .

