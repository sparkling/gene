@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dct: <http://purl.org/dc/terms/> .
@prefix tax: <https://gene.ai/taxonomy/> .

# =============================================================================
# Gene Data Source Taxonomy - SKOS Concept Scheme
# =============================================================================
# A hierarchical classification system for biomedical data sources.
# Structure: Category > Subcategory > Source (3 levels)
# =============================================================================

# -----------------------------------------------------------------------------
# Concept Scheme Definition
# -----------------------------------------------------------------------------

tax:DataSourceTaxonomy a skos:ConceptScheme ;
    skos:prefLabel "Gene Data Source Taxonomy" ;
    rdfs:comment "A hierarchical classification of biomedical data sources organized by domain, subdomain, and individual source." ;
    dct:created "2026-01-25"^^xsd:date ;
    dct:creator "Kurt Cagle" ;
    skos:hasTopConcept
        tax:GeneticsGenomics ,
        tax:CompoundsMolecules ,
        tax:DiseasesPhenotypes ,
        tax:PathwaysNetworks ,
        tax:TraditionalMedicine ,
        tax:NutritionFood ,
        tax:ProteinsMolecularBiology ,
        tax:LiteratureKnowledge ,
        tax:Microbiome .

# =============================================================================
# TOP-LEVEL CATEGORIES (Level 1)
# =============================================================================

tax:GeneticsGenomics a skos:Concept ;
    skos:prefLabel "Genetics and Genomics" ;
    skos:altLabel "01.genetics.genomics" ;
    skos:notation "01" ;
    rdfs:comment "Data sources covering genetic variants, population genetics, pharmacogenomics, and gene expression." ;
    skos:inScheme tax:DataSourceTaxonomy ;
    skos:topConceptOf tax:DataSourceTaxonomy ;
    skos:narrower
        tax:VariantRepositories ,
        tax:FunctionalPrediction ,
        tax:PopulationGenetics ,
        tax:Pharmacogenomics ,
        tax:ExpressionRegulation ,
        tax:CancerGenomics .

