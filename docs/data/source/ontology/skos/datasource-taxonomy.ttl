    skos:prefLabel "Cancer" ;
    skos:altLabel "Cancer Research" , "Oncology" , "Cancer Domain" ;
    skos:notation "cancer" ;
    skos:definition "Facet for categorizing data sources related to cancer research, including genomics, therapeutics, and oncology databases." ;
    skos:inScheme tax:CategoryFacetScheme ;
    skos:topConceptOf tax:CategoryFacetScheme ;
    skos:related tax:CancerGenomics , tax:CancerOncology .

# -----------------------------------------------------------------------------
# Gene Expression Domain Facet
# -----------------------------------------------------------------------------

tax:expression a skos:Concept ;
    skos:prefLabel "Expression" ;
    skos:altLabel "Gene Expression" , "Expression Domain" , "Transcriptomics" ;
    skos:notation "expression" ;
    skos:definition "Facet for categorizing data sources related to gene expression, transcriptomics, eQTL studies, and regulatory elements." ;
    skos:inScheme tax:CategoryFacetScheme ;
    skos:topConceptOf tax:CategoryFacetScheme ;
    skos:related tax:ExpressionRegulation .

# -----------------------------------------------------------------------------
# Format Category Facet
# -----------------------------------------------------------------------------

tax:format a skos:Concept ;
    skos:prefLabel "Format" ;
    skos:altLabel "Data Format" , "Format Category" ;
    skos:notation "format" ;
    skos:definition "Facet for categorizing data sources by their data formats and file types. Links to FormatScheme for specific format types." ;
    skos:inScheme tax:CategoryFacetScheme ;
    skos:topConceptOf tax:CategoryFacetScheme ;
    skos:related tax:FormatScheme .

# -----------------------------------------------------------------------------
# Functional Genomics Domain Facet
# -----------------------------------------------------------------------------

tax:functional a skos:Concept ;
    skos:prefLabel "Functional" ;
    skos:altLabel "Functional Genomics" , "Functional Domain" , "Functional Annotation" ;
    skos:notation "functional" ;
    skos:definition "Facet for categorizing data sources related to functional genomics, including variant impact prediction, gene function, and functional annotations." ;
    skos:inScheme tax:CategoryFacetScheme ;
    skos:topConceptOf tax:CategoryFacetScheme ;
